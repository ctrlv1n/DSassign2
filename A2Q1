#include <iostream>
#include <vector>
using namespace std;
int binary_search_idx(const vector<int>& a, int x){
    int l=0, r=(int)a.size()-1;
    while(l<=r){
        int m=(l+r)/2;
        if(a[m]==x) return m;
        if(a[m]<x) l=m+1; else r=m-1;
    }
    return -1;
}
int main(){ int n; if(!(cin>>n)) return 0; vector<int> a(n); for(int i=0;i<n;i++) cin>>a[i]; int x; cin>>x; cout<<binary_search_idx(a,x); return 0; }
