4A:
#include <stdio.h>
#include <string.h>

int main(){
    char a[100],b[100];
    gets(a); gets(b);
    strcat(a,b);
    puts(a);
}

4B:

#include <stdio.h>
#include <string.h>

int main(){
    char s[100]; gets(s);
    int i=0,j=strlen(s)-1;
    while(i<j){
        char t=s[i];
        s[i]=s[j];
        s[j]=t;
        i++; j--;
    }
    puts(s);
}

4C:

#include <stdio.h>
#include <ctype.h>

int main(){
    char s[100],r[100];
    int i,j=0;
    gets(s);
    for(i=0;s[i];i++){
        char c=tolower(s[i]);
        if(c!='a'&&c!='e'&&c!='i'&&c!='o'&&c!='u')
            r[j++]=s[i];
    }
    r[j]=0;
    puts(r);
}

4D

#include <stdio.h>
#include <string.h>

int main(){
    int n,i,j;
    char s[50][100],t[100];
    scanf("%d",&n);
    for(i=0;i<n;i++) scanf("%s",s[i]);
    for(i=0;i<n;i++)
        for(j=i+1;j<n;j++)
            if(strcmp(s[i],s[j])>0){
                strcpy(t,s[i]);
                strcpy(s[i],s[j]);
                strcpy(s[j],t);
            }
    for(i=0;i<n;i++) puts(s[i]);
}

4E:

#include <stdio.h>
#include <ctype.h>

int main(){
    char c;
    scanf("%c",&c);
    if(c>='A'&&c<='Z') c=tolower(c);
    printf("%c",c);
}




