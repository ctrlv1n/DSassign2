    for(i=0;i<n;i++)
        for(j=i;j<n;j++)
            scanf("%d",&a[k++]);
}


E:

#include <stdio.h>

int main(){
    int n,i,j,k=0;
    scanf("%d",&n);
    int a[n*(n+1)/2];
    for(i=0;i<n;i++)
        for(j=0;j<=i;j++)
            scanf("%d",&a[k++]);
}

