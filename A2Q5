A:
#include <stdio.h>

int main(){
    int n,i;
    scanf("%d",&n);
    int d[n];
    for(i=0;i<n;i++) scanf("%d",&d[i]);
    for(i=0;i<n;i++){
        for(int j=0;j<n;j++)
            printf("%d ", i==j?d[i]:0);
        printf("\n");
    }
}

B:

#include <stdio.h>

int main(){
    int n,i;
    scanf("%d",&n);
    int t[3*n-2];
    for(i=0;i<3*n-2;i++) scanf("%d",&t[i]);
}


c:

#include <stdio.h>

int main(){
    int n,i,j,k=0;
    scanf("%d",&n);
    int a[n*(n+1)/2];
    for(i=0;i<n;i++)
        for(j=0;j<=i;j++)
            scanf("%d",&a[k++]);
}

D:

